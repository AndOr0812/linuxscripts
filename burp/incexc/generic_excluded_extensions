
# Generic excluded extensions

exclude_ext=back
exclude_ext=bak
exclude_ext=bkp
exclude_ext=cache
exclude_ext=chk
exclude_ext=dmp
exclude_ext=dump
exclude_ext=err
exclude_ext=lock
exclude_ext=lockfile
exclude_ext=log
exclude_ext=log1
exclude_ext=log2
exclude_ext=old
exclude_ext=tmp
exclude_ext=temp

# Browser not finished downloads
exclude_ext=download
exclude_ext=crdownload
exclude_ext=part

# Adobe lightroom preview files
exclude_ext=lrprev

# AutoCAD
exclude_ext=dwl
exclude_ext=dwl2
exclude_ext=atmp

# Microsoft Access lock file
exclude_ext=laccdb
exclude_ext=swp

# Microsoft Outlook Exchange sync files
exclude_ext=ost

# Python compiled files
exclude_ext=pyc
exclude_ext=pyo
