exclude_comp=ogg
exclude_comp=mkf
exclude_comp=ac3
exclude_comp=cdda
exclude_comp=cdr
exclude_comp=mpga
exclude_comp=mpg3
exclude_comp=aac
exclude_comp=aac
exclude_comp=flac
