# Microsoft Office lock files (eg */~$somefile.doc)
exclude_regex = .*/~\$[^/]+\.[^/]+

# MacOS files
exclude_regex = \.DS_Store

# odrive sync dir
exclude_regex = \.odrive

# generic directories / files
exclude_regex = .*/Cache\.db
exclude_regex = .*/TemporaryFiles

# generic Thumbs files
exclude_regex = .*/Thumbs\.db
exclude_regex = .*/thumbcache[^/]+\.db
exclude_regex = .*/iconcache[^/]+\.db
